@model IEnumerable<Order>

@if(Model.Count() > 0)
{
    @foreach (var item in Model)    @*  hepsini bu sayfa yapmamız sebemi partiada model.lines olmaası *@
    {
        <div class="my-3">
            <partial name="_OrderDetails" model="@item" />
        </div>
    }

   
}
else
{
    <div class="alert alert-info">

        <i class="lead"> Henüz siparis yok  </i>
    </div>
}